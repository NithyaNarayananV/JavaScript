    const numbers = [1,2,-3,4,5,6,7,8];
    
    const output = except(numbers,[1,-4]);
    
    
    console.log('Input : ' + numbers);
    console.log('Output: ' + output);
    
    function except(array, excluded){
        const array1=[];
        for (let i of array)
            if (!excluded.includes(i))
                array1.push(i);
        return array1
    }
