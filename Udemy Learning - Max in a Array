const numbers = [1,2,1,4,5,6,7,8];

console.log('Input : ' + numbers);
const Count = Max(numbers);

console.log('Output: ' + Count);

function Max(array){
    if (array.length==0)
        return 'Empty Array Dude';
    let max= array[0];
    for (i of array){
        if (max < i)
            max=i
    }
    return max;


}

